<matches>
<% items.forEach(function (item) { %>
    <match>
        <<%= item.local %>><%= item.localgoals %></<%= item.local %>>
        <<%= item.away %>><%= item.awaygoals %></<%= item.away %>>
        <numero><%= item.match == null ? "" : item.match %></numero>
    </match>
<% }); %>
</matches>