<% layout('layout') -%>

<div class="container" >


            <div class="row">
                <div class="col-lg-12">
                
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <div class="row actions">
                                <form method="POST" action="/admin/database/createdb">
                                    <input type="text" name="dbname" />
                                    <button type="submit" class="btn btn-default">Create Database</button>
                                </form>
                            </div>
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                                <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                                    <thead>
                                            <tr>
                                                <th class="sorting" tabindex="0" aria-controls="dataTables-example" rowspan="1" colspan="1" aria-label="Name: activate to sort column ascending">Name</th>
                                                <th class="sorting" tabindex="0" aria-controls="dataTables-example" rowspan="1" colspan="1" aria-label="Status: activate to sort column ascending">Status</th>
                                                <th class="sorting" tabindex="0" aria-controls="dataTables-example" rowspan="1" colspan="1" aria-label="Actions: activate to sort column ascending">Actions</th>

                                            </tr>
                                    </thead>
                                    <tbody>
                                    <% if (items) { %>
                                        <% items.forEach(function (item) { %>
                                            <tr>
                                                <td><%= item %></td>
                                                <td>
                                                <% if (item == currentdbname) { %>
                                                <span class="label label-success">Current</span>
                                                <% } %>
                                                </td>
                                                <td>
                                                <a class="btn btn-primary btn-xs" href="/admin/database/usedb/<%= item %>">Use DB</a>
                                                <a class="btn btn-danger btn-xs" href="/admin/database/cleardb/<%= item %>">Clear DB</a>
                                                </td>
                                            </tr>
                                        <% }); %>
                                    <% } %>
                                    </tbody>
                                </table>
                            </div>
                            <!-- /.table-responsive -->
                        </div>
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-12 -->

    




